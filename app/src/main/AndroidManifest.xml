        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.androidStudyJams.record_attendance.SplashScreenActivity" android:theme="@style/splaceTheme" android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.androidStudyJams.record_attendance.MainActivity">

        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>